  </select>
  <select id="selectNoticeByKeyWord" resultType="com.sweep.pojo.Notice">
    SELECT <include refid="Base_Column_List"></include>
    FROM notice WHERE noticetitle like concat(concat('%',#{keyword}),'%')
    or noticeperson like concat(concat('%',#{keyword}),'%')
    or noticedate like concat(concat('%',#{keyword}),'%')
  </select>
  <insert id="insert" parameterType="com.sweep.pojo.Notice">
