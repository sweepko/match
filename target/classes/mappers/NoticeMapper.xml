      noticedate)
    values (#{noticeid,jdbcType=INTEGER},#{noticeperson,jdbcType=VARCHAR}, #{noticetitle,jdbcType=VARCHAR}, #{noticecontent,jdbcType=VARCHAR},
      #{noticedate,jdbcType=VARCHAR})
  </insert>
  <delete id="deleteByPrimaryKeys">
    delete from notice
    where noticeid in (${ids})
  </delete>
